<tr>
  <td class="league_users">
	    <tr>
	    	<td>Game # <%= game.id %></td>
			<td>Game Date <%= game.game_date %></td>
	    </tr>
		<tr>
			<td><%= link_to "Show", game %></td>
		    <td><%= link_to "Edit", edit_game_path(game) %></td>
		    <td><%= button_to "Destroy", game, :confirm => 'Are you sure?',
						:method => :delete %></td>
		</tr>
  </td>
</tr>